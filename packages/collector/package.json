{
  "name": "@agent-trace/collector",
  "version": "0.1.0",
  "private": true,
  "description": "Strict collector core for ingesting and deduplicating agent-trace events.",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "node scripts/clean-deps.cjs && node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
    "build:deps": "npm run --workspace @agent-trace/schema build",
    "typecheck:deps": "npm run --workspace @agent-trace/schema build",
    "build": "npm run build:deps && npm run clean && node ../../scripts/run-tsc.cjs -p tsconfig.json && node scripts/link-deps.cjs",
    "typecheck": "npm run typecheck:deps && node ../../scripts/run-tsc.cjs -p tsconfig.json --noEmit",
    "test:unit": "npm run build && node --test dist/test/*.test.js",
    "test:manual": "npm run build && node dist/scripts/manual-smoke.js",
    "start": "npm run build && node dist/src/cli.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0"
  }
}
