{
  "name": "@agent-trace/dashboard",
  "version": "0.1.0",
  "private": true,
  "description": "Strict dashboard-core mappers and analytics models for agent-trace.",
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "node -e \"require('node:fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && node ../../scripts/run-tsc.cjs -p tsconfig.json",
    "build:web": "next build",
    "typecheck": "node ../../scripts/run-tsc.cjs -p tsconfig.json --noEmit",
    "typecheck:web": "next build",
    "test:unit": "npm run build && node --test dist/test/*.test.js",
    "test:manual": "npm run build && node dist/scripts/manual-smoke.js",
    "dev:web": "next dev",
    "start:web": "next start",
    "start": "npm run start:web -- --hostname 0.0.0.0 --port 3100"
  },
  "dependencies": {
    "next": "^16.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
